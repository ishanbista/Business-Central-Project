enum 50100 "Leave Status"
{
    Extensible = true;
    
    value(0; "")
    {
        Caption = '';
    }
    value(1; Pending)
    {
        Caption = 'Pending';
    }
    value(2; Approved)
    {
        Caption = 'Approved';
    }
    value(3; Rejected)
    {
        Caption = 'Rejected';
    }
}

